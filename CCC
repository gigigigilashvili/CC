#include <stdio.h>

#include <math.h>

int main() {

    int k,z,i,c;

    float y;

for (i=0;i<7;i++)

{

    printf("\n შეიყვანე Z,K,C:  ");

    scanf("%d",&z);

    scanf("%d",&k);

    scanf("%d",&c);

if (k==z)

    {

        printf("skipped");

    continue;

    }

else if (k+z<10)

{

    switch (k%z)

        {

            case 0:y=pow(y,2);

                printf("nashti aris 0, Y= %f",y);

                    break;

            case 1:y=cos(cos(pow(k,2)));

                printf("nashti aris 1, Y= %f",y);

                    break;

            case 2:y=z;

                printf("nashti aris 2, Y= %f",y);

                    break;

        }

}

else if (k+z>c)

    {

        y=z;

        printf("Y=Z, Y=%f",y);

    }

else if (k+z==c)

    {

        y=sqrt(pow(k,5)+2*pow(z,3));

        printf("%f",y);

    }

}

    return 0;

}



