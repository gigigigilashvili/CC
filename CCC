#include <stdio.h>

#include <stdlib.h>

#include <time.h>


int main()

{

    int array[5][5],i,j,a=0;

    srand(time(0));

    for (i=1;i<6;i++)

    {

        for(j=1;j<6;j++)

        {

            array[i][j] = 1+rand()%10;

            printf("%d\t\t", array[i][j]);



            if (i==j || i+j==4)

                {

                    a=+array[i][j];

                    printf("დიაგონალების ჯამი არის %d",a);

                }

        }

    }



    return 0;

}
